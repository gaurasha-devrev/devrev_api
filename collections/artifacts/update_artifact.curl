#!/bin/bash
# Update artifact metadata

curl -X POST "https://{{base_url}}/artifacts.update" \
  -H "Authorization: Bearer {{DEVREV_TOKEN}}" \
  -H "Content-Type: application/json" \
  -d '{
    "id": "{{artifact_id}}",
    "file": {
      "description": "[UPDATED] Updated description for test document - now includes additional context and metadata"
    }
  }'
