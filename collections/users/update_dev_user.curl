#!/bin/bash
# Update dev user details

curl -X POST "https://{{base_url}}/dev-users.update" \
  -H "Authorization: Bearer {{DEVREV_TOKEN}}" \
  -H "Content-Type: application/json" \
  -d '{
    "id": "{{dev_user_id}}",
    "full_name": "Updated Developer Name",
    "display_name": "UpdatedDev",
    "phone_numbers": ["+1-555-111-2222"]
  }'
