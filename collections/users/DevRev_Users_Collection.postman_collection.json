							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    if (response.dev_users && response.dev_users.length > 0) {",
									"        pm.environment.set('first_dev_user_id', response.dev_users[0].id);",
									"        console.log('Found', response.dev_users.length, 'dev users');",
									"    }",
