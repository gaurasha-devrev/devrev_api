				{
					"name": "Create Dev User",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 201) {",
									"    const response = pm.response.json();",
									"    if (response.dev_user) {",
									"        pm.environment.set('dev_user_id', response.dev_user.id);",
									"        pm.environment.set('dev_user_display_id', response.dev_user.display_id);",
									"        pm.environment.set('dev_user_email', response.dev_user.email);",
									"        console.log('Dev user created with ID:', response.dev_user.id);",
									"    }",
									"} else {",
									"    console.log('Dev user creation failed:', pm.response.text());",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{aat}}",
								"type": "text"
							},
							{
								"key": "Content-Type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"email\": \"{{$randomEmail}}\",\n  \"full_name\": \"{{$randomFullName}}\",\n  \"display_name\": \"{{$randomFirstName}} {{$randomLastName}}\",\n  \"phone\": \"{{$randomPhoneNumber}}\"\n}"
						},
						"url": {
