#!/bin/bash
# Create a new tag

curl -X POST "https://{{base_url}}/tags.create" \
  -H "Authorization: Bearer {{DEVREV_TOKEN}}" \
  -H "Content-Type: application/json" \
  -d '{
    "name": "test-tag",
    "description": "Test tag created via API for categorization and labeling",
    "color": "#3498db"
  }'
