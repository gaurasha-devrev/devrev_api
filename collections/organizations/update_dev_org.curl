#!/bin/bash
# Update dev organization

curl -X POST "https://{{base_url}}/dev-orgs.update" \
  -H "Authorization: Bearer {{DEVREV_TOKEN}}" \
  -H "Content-Type: application/json" \
  -d '{
    "id": "{{dev_org_id}}",
    "display_name": "Updated Dev Organization",
    "description": "Development organization updated via API"
  }'
