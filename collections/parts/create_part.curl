#!/bin/bash
# Create a new part (product/component)

curl -X POST "https://{{base_url}}/parts.create" \
  -H "Authorization: Bearer {{DEVREV_TOKEN}}" \
  -H "Content-Type: application/json" \
  -d '{
    "name": "Test Product",
    "description": "Test product created via API",
    "type": "product",
    "artifacts": []
  }'
